<!-- Footer -->
<footer id="footer">
    <div class="inner" style="text-align: center">
        <ul class="icons">
            {%- for social in site.social_urls -%}
                <li>
                    <a href="{{ social[1] }}" class="icon alt fa fa-{{ social[0] }}" title="{{ social[0] | capitalize }}"></a>
                </li>
            {%- endfor -%}
            <li>
                <a class="icon alt fa fa-feed" href="/feed.xml" title="RSS Feed"></a>
            </li>
        </ul>
        <ul class="copyright">
            <li>&copy;
                {{ site.title }}</li>
            <li>
                Original Design:
                <a href="https://html5up.net/forty">HTML5 UP</a>
            </li>
        </ul>
    </div>
</footer>
